<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mgs1982.remindset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splashScreen.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.ui.LoginActivity"/>
        <activity android:name=".home.HomeScreenActivity"/>
        <activity android:name=".registration.ui.RegistrationActivity"/>
        <activity android:name=".fetchdata.ui.DisplayDataFromFB"/>
    </application>
</manifest>